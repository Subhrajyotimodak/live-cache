{
  "name": "@live-cache/react",
  "version": "0.0.1",
  "description": "React bindings for live-cache",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.umd.js",
      "default": "./dist/index.mjs"
    }
  },
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build": "npm run clean && rollup -c && tsc -p tsconfig.json --emitDeclarationOnly",
    "dev": "rollup -c -w"
  },
  "sideEffects": false,
  "keywords": [
    "javascript",
    "typescript",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=16.8"
  },
  "dependencies": {
    "@live-cache/core": "0.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-sucrase": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "rollup": "^4.9.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  }
}
