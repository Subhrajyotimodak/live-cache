{
  "name": "@live-cache/core",
  "version": "0.0.1",
  "description": "Core data structures and controller APIs for live-cache",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "browser": "./dist/index.umd.js",
      "default": "./dist/index.mjs"
    }
  },
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "build": "npm run clean && rollup -c",
    "dev": "rollup -c -w"
  },
  "sideEffects": false,
  "keywords": [
    "javascript",
    "typescript",
    "library"
  ],
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "rollup": "^4.9.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Subhrajyotimodak/live-cache.git"
  },
  "bugs": {
    "url": "https://github.com/Subhrajyotimodak/live-cache/issues"
  },
  "homepage": "https://github.com/Subhrajyotimodak/live-cache/library/core#readme"
}
